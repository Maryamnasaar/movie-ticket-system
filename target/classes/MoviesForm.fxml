<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<?import javafx.scene.control.TextField?>
<fx:root prefHeight="400.0" prefWidth="600.0" type="AnchorPane" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.moviebookingsystem.MoviesForm">
   <children>
      <Button fx:id="ComingSoonButton" layoutX="12.0" layoutY="109.0" mnemonicParsing="false" onAction="#onNavClick" prefHeight="26.0" prefWidth="87.0" style="-fx-background-color: transparent;" text="coming soon" textOverrun="CLIP" />
      <Button fx:id="PlayingNowButton" layoutX="12.0" layoutY="167.0" mnemonicParsing="false" onAction="#onNavClick" prefHeight="26.0" prefWidth="87.0" style="-fx-background-color: transparent;" text="playing now" />
      <Button fx:id="TrendingButton" layoutX="8.0" layoutY="226.0" mnemonicParsing="false" onAction="#onNavClick" prefHeight="26.0" prefWidth="94.0" style="-fx-background-color: transparent;" text="Trending" />
      <MenuButton fx:id="CategoryMenu" layoutX="12.0" layoutY="320.0" mnemonicParsing="false" onAction="#onNavClick" style="-fx-background-color: transparent;" text="Category">
        <items>
          <MenuItem mnemonicParsing="false" text="ACTION" />
          <MenuItem mnemonicParsing="false" text="COMEDY" />
            <MenuItem mnemonicParsing="false" text="habttt" />
        </items>
      </MenuButton>
      <TextField layoutX="102.0" layoutY="18.0" prefHeight="26.0" prefWidth="478.0" />
      <ImageView fitHeight="33.0" fitWidth="38.0" layoutX="66.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: transparent;">
         <image>
            <Image url="@../../../../../Downloads/search.gif" />
         </image>
         <effect>
            <Glow level="0.52" />
         </effect>
      </ImageView>
      <Pane fx:id="comingSoonPage" layoutX="159.0" layoutY="60.0" prefHeight="332.0" prefWidth="430.0">
         <children>
            <Pane fx:id="PlayingNowPage" layoutX="-5.0" layoutY="-5.0" prefHeight="341.0" prefWidth="440.0">
               <children>
                  <Pane fx:id="TrendingPage" prefHeight="341.0" prefWidth="440.0">
                     <children>
                        <Pane fx:id="CategoryPage" layoutY="14.0" prefHeight="326.0" prefWidth="440.0" />
                     </children>
                  </Pane>
               </children>
            </Pane>
         </children>
      </Pane>
   </children>
   <effect>
      <Blend />
   </effect>
</fx:root>
